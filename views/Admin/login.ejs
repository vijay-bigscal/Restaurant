<link rel="stylesheet" type="text/css" href="css/bootstrap.css">
<link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
<link rel="stylesheet" type="text/css" href="css/style.css">
<script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
<script src="js/bootstrap.js" type="text/javascript"></script>
<script src="js/bootstrap.min.js" type="text/javascript"></script>

<style type="text/css">

body{

    background-color:#f5f5f5;
}

</style>

<div class="container">
    <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
            <div class="account-wall">
                <div id="my-tab-content" class="tab-content">
			 	  <div class="tab-pane active" id="login">
	                  <div class="text-center"> 
	                      <img  src="/RestaurantLogo/logo-red.PNG" alt="" width="200px">
	                  </div>
	             		   
     				<form class="form-signin" action="/postAdminLogin" method="post">
         				<input type="text" class="form-control" placeholder="email"  name="email"  type="email" value="admin@foodcorner.com" required autofocus>
         				<input type="password" class="form-control" placeholder="Password" name="password"  value="" required>
         				<input type="submit" class="btn btn-lg btn-default btn-block" value="Log In" />
         			</form>		

     			  <% if( Error != "") { %>
                    <div class="text-center">
                       <b style="color:red"><%= Error %></b>
                    </div>
                  <% } %>
	
				</div>
              </div>
          </div>
      </div>
  </div>
</div>